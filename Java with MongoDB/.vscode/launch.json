{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/server/Basketball/Microservice/bin/Debug/net8.0/NBA-API.dll",
            "args": [],
            "cwd": "${workspaceFolder}/server/Basketball/Microservice",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        },
        {
            "type": "java",
            "name": "Spring Boot-BasketballMicroserviceApplication<basketball-microservice>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "com.nba.basketball_microservice.BasketballMicroserviceApplication",
            "projectName": "basketball-microservice",
            "args": "",
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}